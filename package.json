{
  "name": "novaeditmobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:unit": "jest --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts' --runInBand",
    "test:e2e:build": "detox build",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:performance": "jest --testMatch='**/__tests__/performance/**/*.test.ts' --runInBand --forceExit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:ci": "jest --coverage --watchAll=false --ci --testResultsProcessor=jest-junit",
    "build:android": "expo run:android --variant release",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:aab": "cd android && ./gradlew bundleRelease",
    "build:android:clean": "cd android && ./gradlew clean",
    "build:optimized": "NODE_ENV=production expo build:android --type apk --no-publish",
    "verify:offline": "node scripts/verify-offline-capability.js",
    "optimize:assets": "node scripts/optimize-assets-fixed.js",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "setup:fonts": "react-native link && react-native-asset",
    "generate:icons": "expo install expo-asset && node scripts/generate-app-icons.js",
    "clean": "npm run build:android:clean && rm -rf node_modules/.cache",
    "reset": "npm run clean && npm install && npm run verify:offline",
    "prebuild:android": "npm run build:android:clean",
    "analyze:apk": "npx react-native-bundle-visualizer",
    "prepare-build": "node scripts/prepare-apk-build.js"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/native": "^6.1.17",
    "@tensorflow-models/body-segmentation": "^1.0.2",
    "@tensorflow-models/face-landmarks-detection": "^1.0.2",
    "@tensorflow/tfjs": "^4.20.0",
    "@tensorflow/tfjs-backend-webgl": "^4.20.0",
    "@tensorflow/tfjs-converter": "^4.20.0",
    "@tensorflow/tfjs-react-native": "^0.8.0",
    "@xmldom/xmldom": "^0.9.8",
    "eslint": "^9.28.0",
    "expo": "~51.0.28",
    "expo-av": "~14.0.6",
    "expo-blur": "~13.0.2",
    "expo-camera": "~15.0.13",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.9",
    "expo-gl": "~14.0.2",
    "expo-gl-cpp": "~11.4.0",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-media-library": "~16.0.4",
    "expo-router": "~3.5.23",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "ffmpeg-kit-react-native": "^6.0.2",
    "glob": "^11.0.2",
    "lottie-react-native": "6.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-elements": "^3.4.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.3",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-slider": "^0.11.0",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.2.0",
    "react-native-track-player": "^4.1.1",
    "react-native-vector-icons": "^10.1.0",
    "react-native-video": "^6.4.5",
    "react-native-vision-camera": "^4.5.1",
    "react-native-web": "~0.19.10",
    "rimraf": "^6.0.1",
    "uuid": "^11.1.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.0",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "~18.2.79",
    "detox": "^20.13.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "jest-junit": "^16.0.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.3.3"
  },
  "resolutions": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native": "0.74.5"
  },
  "overrides": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native": "0.74.5"
  },
  "private": true
}
