{
  "name": "novaeditmobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "test:unit": "jest --coverage --watchAll=false",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts' --runInBand",
    "test:e2e:build": "detox build",
    "test:e2e:android": "detox test --configuration android.emu.debug",
    "test:e2e:ios": "detox test --configuration ios.sim.debug",
    "test:performance": "jest --testMatch='**/__tests__/performance/**/*.test.ts' --runInBand --forceExit",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:ci": "jest --coverage --watchAll=false --ci --testResultsProcessor=jest-junit",
    "build:android": "expo run:android --variant release",
    "build:android:apk": "cd android && ./gradlew assembleRelease",
    "build:android:aab": "cd android && ./gradlew bundleRelease",
    "build:android:clean": "cd android && ./gradlew clean",
    "build:optimized": "NODE_ENV=production expo build:android --type apk --no-publish",
    "prepare:offline": "node scripts/prepare-offline-content.js",
    "verify:offline": "node scripts/verify-offline-capability.js",
    "optimize:assets": "node scripts/optimize-assets.js",
    "analyze:bundle": "npx react-native-bundle-visualizer",
    "setup:fonts": "react-native link && react-native-asset",
    "generate:icons": "expo install expo-asset && node scripts/generate-app-icons.js",
    "clean": "npm run build:android:clean && rm -rf node_modules/.cache",
    "reset": "npm run clean && npm install && npm run prepare:offline",
    "prebuild:android": "npm run build:android:clean",
    "analyze:apk": "npx react-native-bundle-visualizer"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/slider": "^4.5.7",
    "@react-navigation/native": "^7.0.14",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-react-native": "^0.8.0",
    "@tensorflow/tfjs-backend-webgl": "^4.15.0",
    "@tensorflow/tfjs-converter": "^4.15.0",
    "@tensorflow-models/body-segmentation": "^1.0.2",
    "@tensorflow-models/face-landmarks-detection": "^1.0.2",
    "@tensorflow-models/hand-pose-detection": "^2.0.0",
    "@tensorflow-models/pose-detection": "^2.0.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@mediapipe/drawing_utils": "^0.3.1675466064",
    "@mediapipe/camera_utils": "^0.3.1675466064",
    "expo": "~52.0.46",
    "expo-av": "^15.1.4",
    "expo-blur": "~14.0.1",
    "expo-camera": "^16.1.6",
    "expo-file-system": "^18.1.10",
    "expo-font": "~13.0.4",
    "expo-gl": "^15.1.5",
    "expo-gl-cpp": "^11.4.0",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-media-library": "^17.1.6",
    "expo-router": "~4.0.20",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "ffmpeg-kit-react-native": "^5.1.0",
    "lottie-react-native": "^7.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-elements": "^3.4.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-paper": "^5.14.5",
    "react-native-progress": "^5.0.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-slider": "^0.11.0",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "^15.12.0",
    "react-native-track-player": "^4.1.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.14.1",
    "react-native-vision-camera": "^4.0.0",
    "react-native-web": "~0.19.13",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "@types/jest": "^29.5.0",
    "@testing-library/react-native": "^12.4.0",
    "@testing-library/jest-native": "^5.4.0",
    "detox": "^20.13.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "jest-junit": "^16.0.0",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.1.0",
    "typescript": "~5.3.3"
  },
  "private": true
}